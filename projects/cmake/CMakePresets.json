{
    "version": 10,
    "cmakeMinimumRequired":
    {
        "major": 3,
        "minor": 31,
        "patch": 0
    },
    "configurePresets":
    [
        {
            "name": "base",
            "generator": "Ninja",
            "binaryDir": "cmake-${presetName}",
            "warnings":
            {
                "unusedCli": false
            }
        },
        {
            "name": "debug",
            "cacheVariables":
            {
                "CORE_DEBUG": "1",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "release",
            "cacheVariables":
            {
                "CORE_RELEASE": "1",
                "CMAKE_BUILD_TYPE": "MinSizeRel"
            }
        },
        {
            "name": "test",
            "inherits": "debug",
            "cacheVariables":
            {
                "CORE_TEST": "1"
            }
        },
        {
            "name": "x86-64",
            "cacheVariables":
            {
                "CORE_X86": "1"
            }
        },
        {
            "name": "arm-64",
            "cacheVariables":
            {
                "CORE_ARM": "1"
            }
        },
        {
            "name": "linux",
            "cacheVariables":
            {
                "CORE_LINUX": "1"
            },
            "vendor":
            {
                "jetbrains.com/clion":
                {
                    "toolchain": "Clang"
                }
            }
        },
        {
            "name": "emscripten",
            "cacheVariables":
            {
                "CORE_EMSCRIPTEN": "1"
            },
            "vendor":
            {
                "jetbrains.com/clion":
                {
                    "toolchain": "Emscripten"
                }
            }
        },
        {
            "name": "linux-x86-64-debug",
            "inherits": ["linux", "x86-64", "debug", "base"]
        },
        {
            "name": "linux-x86-64-release",
            "inherits": ["linux", "x86-64", "release", "base"]
        },
        {
            "name": "linux-x86-64-test",
            "inherits": ["linux", "x86-64", "test", "base"]
        },
        {
            "name": "linux-arm-64-debug",
            "inherits": ["linux", "arm-64", "debug", "base"]
        },
        {
            "name": "linux-arm-64-release",
            "inherits": ["linux", "arm-64", "release", "base"]
        },
        {
            "name": "emscripten-debug",
            "inherits": ["emscripten", "debug", "base"]
        },
        {
            "name": "emscripten-release",
            "inherits": ["emscripten", "release", "base"]
        }
    ],
    "buildPresets":
    [
        {
            "name": "base",
            "configurePreset": "base",
            "hidden": true,
            "nativeToolOptions": ["-k0"]
        },
        {
            "name": "linux_x86_64 (debug)",
            "configurePreset": "linux-x86-64-debug",
            "inherits": "base"
        },
        {
            "name": "linux_x86_64 (release)",
            "configurePreset": "linux-x86-64-release",
            "inherits": "base"
        },
        {
            "name": "linux_x86_64 (test)",
            "configurePreset": "linux-x86-64-test",
            "inherits": "base"
        },
        {
            "name": "linux_arm_64 (debug)",
            "configurePreset": "linux-arm-64-debug",
            "inherits": "base"
        },
        {
            "name": "linux_arm_64 (release)",
            "configurePreset": "linux-arm-64-release",
            "inherits": "base"
        },
        {
            "name": "emscripten (debug)",
            "configurePreset": "emscripten-debug",
            "inherits": "base"
        },
        {
            "name": "emscripten (release)",
            "configurePreset": "emscripten-release",
            "inherits": "base"
        }
    ]
}